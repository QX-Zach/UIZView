<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="personal.ui.lingchen.uizview">

    <uses-feature android:name="android.hardware.autofocus" />
    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:launchMode="singleInstance"
            android:enabled="false"
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <activity-alias
            android:name=".defaultIcon"
            android:enabled="true"
            android:icon="@mipmap/ic_launcher"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name=".iconOne"
            android:enabled="false"
            android:icon="@mipmap/ic_launcher_one"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name=".iconTwo"
            android:enabled="false"
            android:icon="@mipmap/ic_launcher_two"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name=".iconThree"
            android:enabled="false"
            android:icon="@mipmap/ic_launcher_three"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name=".iconFour"
            android:enabled="false"
            android:icon="@mipmap/ic_launcher_four"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity android:name=".UIActivity.ChangeAppIconActivity" android:label="修改App图标"></activity>
        <activity
            android:name=".UIActivity.PageNumIndictorActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".UIActivity.TreeViewActivity" />
        <activity android:name=".UIActivity.PercentCircleActivity" />
        <activity android:name=".UIActivity.UIZRippleIntroActivity" />
        <activity android:name=".UIActivity.ArcMenuActivity" />
        <activity android:name=".UIActivity.CirclePercentActivity" />
        <activity
            android:name=".UIActivity.ZheXianActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".UIListActivity" />


        <activity android:name=".UIActivity.IndicatorProgressActivity" />
        <activity android:name=".LoadingUI.SlackLoading.SlackLoadingActivity" />
        <activity android:name=".UIActivity.WinnowerActivity" />
        <activity android:name=".UIActivity.BezierTestActivity" />
        <activity android:name=".UIActivity.OzerDialActivity" />
        <activity android:name=".UIActivity.RecyleViewActivity" />
        <activity android:name=".UI.Snowflake.SnowFlokeActivity" />
        <activity android:name=".UIActivity.SeekBarActivity" />
        <activity android:name=".UIActivity.OznerTempDialActivity" />
        <activity android:name=".UIActivity.RealTempTestActivity" />
        <activity android:name=".UIActivity.ArcHeaderViewActivity" />
        <activity android:name=".UIActivity.CameraTestActivity" />
        <activity android:name=".UIActivity.GradientTestActivity" />
        <activity android:name=".UIActivity.WeatherActivity" />
    </application>

</manifest>