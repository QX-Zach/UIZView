<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="personal.ui.lingchen.uizview">

    <uses-feature android:name="android.hardware.autofocus" />
    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UIActivity.TreeViewActivity"></activity>
        <activity android:name=".UIActivity.PercentCircleActivity" />
        <activity android:name=".UIActivity.UIZRippleIntroActivity" />
        <activity android:name=".UIActivity.ArcMenuActivity" />
        <activity android:name=".UIActivity.CirclePercentActivity" />
        <activity
            android:name=".UIActivity.ZheXianActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".UIListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UIActivity.IndicatorProgressActivity" />
        <activity android:name=".LoadingUI.SlackLoading.SlackLoadingActivity" />
        <activity android:name=".UIActivity.WinnowerActivity" />
        <activity android:name=".UIActivity.BezierTestActivity" />
        <activity android:name=".UIActivity.OzerDialActivity" />
        <activity android:name=".UIActivity.RecyleViewActivity" />
        <activity android:name=".UI.Snowflake.SnowFlokeActivity" />
        <activity android:name=".UIActivity.SeekBarActivity" />
        <activity android:name=".UIActivity.OznerTempDialActivity" />
        <activity android:name=".UIActivity.RealTempTestActivity" />
        <activity android:name=".UIActivity.ArcHeaderViewActivity" />
        <activity android:name=".UIActivity.CameraTestActivity" />
        <activity android:name=".UIActivity.GradientTestActivity" />
        <activity android:name=".UIActivity.WeatherActivity" />
    </application>

</manifest>