<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="personal.ui.lingchen.uizview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".UIActivity.IndicatorProgressActivity"/>
        <activity android:name=".LoadingUI.SlackLoading.SlackLoadingActivity">
        </activity>
        <activity android:name=".UIActivity.WinnowerActivity">
        </activity>
        <activity android:name=".UIActivity.BezierTestActivity">
        </activity>
        <activity android:name=".UIActivity.OzerDialActivity">
        </activity>
        <activity android:name=".UIActivity.RecyleViewActivity">
        </activity>
        <activity android:name=".UI.Snowflake.SnowFlokeActivity">
        </activity>
        <activity android:name=".UIActivity.SeekBarActivity">
        </activity>
        <activity android:name=".UIActivity.OznerTempDialActivity">
        </activity>
        <activity android:name=".UIActivity.RealTempTestActivity">
        </activity>
        <activity android:name=".UIActivity.ArcHeaderViewActivity">
        </activity>
        <!-- This service is only used on devices with API v17+ -->
        <activity
            android:name=".UIActivity.ItemListActivity"
            android:label="@string/title_item_list"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".UIActivity.ItemDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName=".UIActivity.ItemListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="personal.ui.lingchen.uizview.UIActivity.ItemListActivity"/>
        </activity>
    </application>

</manifest>